//n을 입력받으면 피보나치 수열의 n번째 원소를 출력하라.
#include <stdio.h>

int fibo(int n){
  int arr[10]; //배열 사이즈 없으면 에러, CPP는 없어도 된다는 거...
  int i;
  arr[0]=0; //기본선언
  arr[1]=1;

  for(i=2; i<=n; i++){
    arr[i]=arr[i-1]+arr[i-2]; //피보나치 수열의 처음부터 계산을 한다.
    if(i==n)
      return arr[i];
  }
  return arr[n];
}

int main(){
  int n=5;
  printf("input %d, result %d" ,n,fibo(n));
}
